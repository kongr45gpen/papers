@article{peterson_when_2021,
	title = {When Worlds Collide --- A comparative analysis of issues impeding adoption of Agile for hardware},
	volume = {1},
	issn = {2732-527X},
	url = {http://www.cambridge.org/core/journals/proceedings-of-the-design-society/article/when-worlds-collide-a-comparative-analysis-of-issues-impeding-adoption-of-agile-for-hardware/6A4FF14A1D3313DE07D0330C9976CAE1},
