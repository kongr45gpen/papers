@inproceedings{schoutetens_chipsats_2020,
	title = {{ChipSats} -New Opportunities},
	abstract = {Future, sustainable space exploration and utilization can be realized through {ChipSats}. {ChipSat} design modularity and low development costs lower the barrier of entry to space and offer potential new applications. Informed by an extensive review of existing literature, this paper establishes an interdisciplinary focus on the state-of-the-art technologies and compelling opportunities of {ChipSats}. Despite minimal functionality, short orbit lifetimes, and reduced regulation, {ChipSats} offer progression over present capabilities. This research is centered around cross-cutting considerations for the International Space University ({ISU}) to develop and launch {ChipSats} within five years. Existing technologies permit low-functionality payloads, where a 'Sputnik-like' mission with elementary communication functionalities serves as a pertinent precursor for {ISU}'s {ChipSat} mission. The delivery and launch of {ISU}'s own {ChipSat} through the proposed roadmap leans on three pillars: (1) the introduction of an {ISU} {ChipSat} laboratory; (2) regulatory guidance to outline the acceptable boundary conditions for {ChipSat} delivery and operation; and (3) space education and outreach. Technology gaps restrict payload selection, which impedes alternative space-borne applications, and persisting legal hurdles require resolution. A sequence of recommendations addresses these challenges and endorses the development of nanotechnology for component miniaturization.},
	author = {Schoutetens, Frederic and Carrillo Flores, Andrea and Marsh, Marco and Gupta, Taavishe and Sarkar, Shreya and Tiballi, Christine and Tanouti, Iliass},
	date = {2020-10-13},
}

@article{gong_design_2022,
	title = {Design of foldable {PCBSat} enabling three-axis attitude control},
	volume = {192},
	issn = {0094-5765},
	url = {https://www.sciencedirect.com/science/article/pii/S0094576521006391},
	doi = {10.1016/j.actaastro.2021.12.004},
	abstract = {Satellite-on-a-printed circuit board ({PCBSat}) is a kind of femto-satellite that integrates all the electronic devices onto a {PCB}. Constrained by the energy/volume capacity, traditional attitude actuators no longer suit the {PCBSat}. In the paper, employing the flexible {PCB} material and micro-steppers, a new design that enables the {PCBSat} to fold and unfold is proposed, and a prototype is manufactured. The foldable {PCBSat} could achieve attitude maneuver by the spatial multibody dynamical effect. Meanwhile, the copper coils embedded in the boards can form a 3D magnetorquer when the {PCBSat} is at the orthogonal-fold status, making magnetic de-tumbling possible. Related theories combined with the detailed design of the {PCBSat} are introduced, and simulations are conducted to demonstrate the effectiveness of the attitude control mechanism.},
	pages = {291--300},
	journaltitle = {Acta Astronautica},
	shortjournal = {Acta Astronautica},
	author = {Gong, Haoran and Gong, Shengping},
	urldate = {2022-02-22},
