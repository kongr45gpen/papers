	keywords = {Companies, Conferences, Data mining, Lenses, Safety, Safety-critical systems, Software, Standards, agile, continuous delivery, continuous deployment, continuous integration, systematic mapping study},
}

@article{sweeting_modern_2018,
	title = {Modern Small Satellites-Changing the Economics of Space},
	volume = {106},
