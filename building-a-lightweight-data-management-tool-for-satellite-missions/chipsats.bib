@inproceedings{bell_potential_2012,
	title = {The potential of miniature electrodynamic tethers to enhance capabilities of femtosatellites},
	doi = {10.1109/PLASMA.2012.6383597},
	abstract = {The emerging interest in nanospacecraft (1-10 kg) has generated interest in exploring the potential for even smaller spacecraft, both as stand-alone satellites or as a distributed swarm. Because of advances in integrated circuit and microelectromechanical systems ({MEMS}) technology, the feasibility of miniaturized spacecraft at the levels of fully monolithic semiconductor integrated circuits (10-100 mg) or hybrid integrated circuits (10-100 g) is being seriously investigated. Effectively, this architecture can be thought of as a small ‚Äúsatellite-on-a-chip,‚Äù or {ChipSat}. {ChipSats} belong to the picosatellite (100 g-1 kg) and femtosatellite ({\textless};100 g) mass categories. However, flat {ChipSat} wafers can have an inherently high area-to-mass ratio. Although this feature can be exploited for new behaviors, it can result in an undesirably short orbital life in low Earth orbit ({LEO}) due to atmospheric drag, ranging from a few weeks to a few hours.},
	eventtitle = {2012 Abstracts {IEEE} International Conference on Plasma Science},
	pages = {2P--41--2P--41},
	booktitle = {2012 Abstracts {IEEE} International Conference on Plasma Science},
	author = {Bell, Iverson C. and Gilchrist, Brian E. and Bilen, Sven G. and {McTernan}, Jesse K.},
