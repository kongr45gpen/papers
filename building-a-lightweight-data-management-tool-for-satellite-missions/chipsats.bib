	keywords = {Black-box model, Communication systems, {CubeSat}, Physical layer, {SysML}, System analysis and design, Systems architecture, Visible light communication, intersatellite communication ({ISC}), model-based systems engineering ({MBSE}), physical layer ({PL}), system architecture, visible light communication ({VLC})},
