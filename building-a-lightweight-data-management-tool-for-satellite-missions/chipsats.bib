	date = {2022-01},
	langid = {english},
	note = {Number: 5
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {Cram√©r-Rao Bound ({CRB}), {CubeSat}, femto-satellite, hybrid measurements, passive source localization},
}

