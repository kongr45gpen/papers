@article{honore-livermore_agile_2021-1,
	title = {An Agile Systems Engineering Analysis of Socio-technical Aspects of a University-built {CubeSat}},
	volume = {24},
	rights = {¬© 2021 by Evelyn Honor√©-Livermore, Joseph L. Garrett, Ron Lyells, Robert Angier, and Bob Epps. Published by {INCOSE} with permission.},
	issn = {2156-4868},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/inst.12344},
	doi = {10.1002/inst.12344},
	abstract = {System development teams often face challenges outside of pure technical problems. There are unpredictable circumstances in many areas including customer needs, personnel changes, and sociotechnical issues that can span across the development organization such as knowledge management and goal alignment. This paper presents the results of an exploratory case study on a university {CubeSat} team developing an Earth Observation satellite. Formal analysis of agile systems engineering helps improve success throughout the {CubeSat} lifecycle. We apply the {INCOSE} Agile systems engineering {WG} decision guidance method for applying agile system engineering method to identify areas in which the project organization can improve to become more agile in three specific problem spaces: customer problem space, solution space, and product development space. The analysis process led to valuable insights about how the project organization of an academic project differs from that of industry. Additionally, the results indicate that areas such as stakeholder management and support environment could be factors that would benefit more from agile responsiveness.},
	pages = {21--26},
	number = {3},
	journaltitle = {{INSIGHT}},
	author = {Honor√©-Livermore, Evelyn and Garrett, Joseph L. and Lyells, Ron and Angier, Robert (Rock) and Epps, Bob},
	urldate = {2024-08-02},
	date = {2021},
	langid = {english},
	note = {\_eprint: https://incose.onlinelibrary.wiley.com/doi/pdf/10.1002/inst.12344},
}

@article{honore-livermore_academics_2022,
	title = {Academics' perception of systems engineering and applied research projects},
	volume = {25},
	rights = {¬© 2021 The Authors. Systems Engineering published by Wiley Periodicals {LLC}},
	issn = {1520-6858},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/sys.21599},
	doi = {10.1002/sys.21599},
	abstract = {There is an increased complexity in applied research projects that demand more researcher skills, especially in managing the research project and interdisciplinary work. Researchers receive little training in how to manage such projects, yet most manage to deliver project results. There is a tradition of project management and systems engineering which benefits complex development projects in industrial settings. Despite the apparent benefits, we found limited application of either project management or systems engineering practices in academia. Furthermore, we found barriers to applying these practices in the first place, such as a lack of clear guidance or tools for their execution. A case study based on 18 semi-structured interviews provides a perspective on academic research projects, and how the application of project management and systems engineering in an academic setting shows promise to improve the realization of concept design.},
	pages = {19--34},
	number = {1},
	journaltitle = {Systems Engineering},
	author = {Honor√©-Livermore, Evelyn and Fossum, Knut Robert and Veitch, Erik},
	urldate = {2024-08-02},
	date = {2022},
	langid = {english},
	note = {\_eprint: https://incose.onlinelibrary.wiley.com/doi/pdf/10.1002/sys.21599},
	keywords = {good-paper, project management, projects, research, systems engineering},
}

