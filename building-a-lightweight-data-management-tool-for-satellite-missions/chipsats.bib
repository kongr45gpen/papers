@article{li_lensless_2024,
	title = {Lensless camera: Unraveling the breakthroughs and prospects},
	issn = {2667-3258},
	url = {https://www.sciencedirect.com/science/article/pii/S2667325824001328},
	doi = {10.1016/j.fmre.2024.03.019},
	shorttitle = {Lensless camera},
	abstract = {Lensless imaging is an innovative and swiftly advancing technology at the intersection of optics, imaging technology, and computational science. It captures scene by directly recording the interference or diffraction patterns of light, subsequently utilizing algorithms to reconstruct the original image from these patterns. Lensless imaging transforms traditional imaging paradigms, offering newfound design flexibility and the capacity to seamlessly integrate within diverse imaging ecosystems. This paper aims to provide an overview of significant developments in optical modulation elements, image sensors, and reconstruction algorithms. The novel application scenarios that benefit from lensless computational imaging are presented. The opportunities and challenges associated with lensless camera are discussed for further improving its performance.},
