@article{bruno_simulation_2014,
	title = {Simulation of Heat Transfer in Wafer-Integrated Femtosatellites},
	volume = {51},
	doi = {10.2514/1.A32517},
	abstract = {A study was conducted to simulate heat transfer in wafer-integrated femtosatellites. The orbit was assumed to be circular and to cross the equator between Earth and the sun. This would correspond to worst-case maximum eclipse orbits with given orbital angular velocity. An Eulerian integration was used over the radiation heat transfer equation to obtain the temperature at each time step after the view factors were calculated. Investigations revealed that the thermal balance of the system depended on the view factors, the energy converted by the solar cells into electricity, the rate at which the battery absorbs electricity and released it to the electronics, and the absorptivity and emissivity of the surfaces. The emissivity values were based on the values for solar cells instead of the values for crystalline silicon, as most of the usable surface was covered by the solar cells.},
